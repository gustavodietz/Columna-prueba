	Pmid = {22760489},
	Rating = {0},
	Title = {{Preoperative anxiety and catastrophizing: a systematic review and meta-analysis of the association with chronic postsurgical pain.}},
	Uri = {\url{papers3://publication/doi/10.1097/AJP.0b013e31824549d6}},
